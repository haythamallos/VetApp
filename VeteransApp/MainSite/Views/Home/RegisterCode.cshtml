@{
    Layout = "~/Views/_LayoutHome.cshtml";
    ViewData["Title"] = "Veteranpp Register Code";
    ViewData["Description"] = "Veteranapp Register Code";
}

<section id="wrapper" class="login-register">
    <div class="login-box login-sidebar">
        <div class="white-box">
            <form class="form-horizontal form-material" id="loginform" action="index.html">
                <a href="javascript:void(0)" class="text-center db"><img src="~/plugins/images2/banner-308x100.png" alt="Home" /></a>
                <h3 class="box-title m-t-40 m-b-0">Please Enter Your Registration Code</h3><small>An email has been sent to you.  Please enter the code stated in the email.</small>
                <div class="form-group m-t-20">
                    <div class="col-xs-12">
                        <input class="form-control" type="text" required="" placeholder="Registeration Code">
                    </div>
                </div>
                <div class="form-group text-center m-t-20">
                    <div class="col-xs-12">
                        <button class="btn btn-info btn-lg btn-block text-uppercase waves-effect waves-light" type="submit">Get Started</button>
                    </div>
                </div>
                <div class="form-group m-b-0">
                    <div class="col-sm-12 text-center">
                        <p>Already have an account? <a href="/Home/Login2" class="text-primary m-l-5"><b>Sign In</b></a></p>
                    </div>
                </div>
            </form>
        </div>
    </div>
</section>


