                                @Html.DropDownListFor(x => x.S203, (SelectList)ViewBag.MovementList140Deg)

                                <br />
                                <span>Extension</span>
                                @Html.DropDownListFor(x => x.S202, (SelectList)ViewBag.MovementList140Deg)
