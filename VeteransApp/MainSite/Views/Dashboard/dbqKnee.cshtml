                                    <span> or </span>
                                </div>
                                <div class="col-md-7">
                                    @Html.DropDownListFor(x => x.VarianceFlareUps, (SelectList)ViewBag.VarianceFlareUps, new { @onchange = "onChangeFlareUps(this.options[this.selectedIndex].innerHTML);", style = "width: 600px; font-size:90%;" })
