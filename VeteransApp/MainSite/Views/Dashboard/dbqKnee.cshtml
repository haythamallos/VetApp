                                    @Html.RadioButtonFor(x => x.S115Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S115Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S109) Knee instability
                                    </label>
                                    @Html.RadioButtonFor(x => x.S109Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S109Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S109Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S103) Patellar dislocation
                                    </label>
                                    @Html.RadioButtonFor(x => x.S103Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S103Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S103Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
