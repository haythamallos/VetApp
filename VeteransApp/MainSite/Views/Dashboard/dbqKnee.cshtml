                                    @Html.RadioButtonFor(x => x.S64Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S64Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S65) Knee fracture (including patellar fracture)
                                    </label>
