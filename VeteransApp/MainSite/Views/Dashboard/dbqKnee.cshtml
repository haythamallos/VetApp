                                    </label>
                                    @Html.RadioButtonFor(x => x.S79Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S79Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S79Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S127) Tibia and/or Fibula fracture
                                    </label>
                                    @Html.RadioButtonFor(x => x.S127Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S127Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S127Side, "BOTH") Both
                                </div>
