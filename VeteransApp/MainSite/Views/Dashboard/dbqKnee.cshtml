                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S57) Knee strain
                                    </label>
                                    @Html.RadioButtonFor(x => x.S57Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S57Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S57Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S58) Knee tendonitis/tendonosis
                                    </label>
                                    @Html.RadioButtonFor(x => x.S58Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S58Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S58Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S59) Knee meniscal tear
                                    </label>
