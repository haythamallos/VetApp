                                    @Html.RadioButtonFor(x => x.S165Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S165Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S165Side, "BOTH") Both
                                    <br />
