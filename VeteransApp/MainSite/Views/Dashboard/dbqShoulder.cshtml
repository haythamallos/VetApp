                                    </div>
                                    <div class="col-md-7">
                                        @Html.TextAreaFor(x => x.S17, new { htmlAttributes = new { @class = "form-control form-control-line", placeholder = "enter your text here ..." }, style = "width: 100%; height: 100px" })
                                    </div>
                                </div>

                                <label class="font-bold">What is the diagnosis of your service-connected shoulder condition? Check if applies and choose Right, Left or Both?</label>
