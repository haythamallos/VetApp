                                    @Html.RadioButtonFor(x => x.S79Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S79Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S80) Ankylosis Of Elbow Joint
                                    </label>
                                    @Html.RadioButtonFor(x => x.S80Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S80Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S80Side, "BOTH") Both
                                </div>
                              
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        Other (specify):
                                    </label>
                                    @Html.RadioButtonFor(x => x.S81Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S81Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S81Side, "BOTH") Both
                                    <br />
                                    @Html.EditorFor(x => x.S81Other, new { htmlAttributes = new { style = "width: 600px" } })

                                </div>


                            </div>
                            <hr />
                            <div class="form-check form-check-inline">
                                <hr />
                                <div class="col-md-5">
                                    <span>Describe the history:  </span>
                                </div>
                                <div class="col-md-7">
                                    @Html.EditorFor(x => x.VarianceHistoryWriteIn, new { htmlAttributes = new { style = "width: 600px" } })
                                </div>
                                <hr />
                                <div class="col-md-5">
                                    <span> or </span>
                                </div>
                                <div class="col-md-7">
                                    @Html.DropDownListFor(x => x.VarianceHistory, (SelectList)ViewBag.VarianceHistory, new { @onchange = "onChangeHistory(this.options[this.selectedIndex].innerHTML);", style = "width: 600px; font-size:90%;" })
                                </div>
                                <div class="col-md-12">
                                </div>

                            </div>

                            <div class="form-check form-check-inline">
                                <hr />
                                <div class="col-md-5">
                                    <span>Does the veteran report flare-ups impact function?  </span>
                                </div>
                                <div class="col-md-7">
