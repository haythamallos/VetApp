        }
        function validate_4() {
            var isValid = true;

            return isValid;
        }

        function setROM() {
            var S64Side = $('input[name=S72Side]:checked').val();
            var S65Side = $('input[name=S73Side]:checked').val();
            var S66Side = $('input[name=S74Side]:checked').val();
            var S67Side = $('input[name=S75Side]:checked').val();
            var S68Side = $('input[name=S76Side]:checked').val();
            var S69Side = $('input[name=S77Side]:checked').val();
            var S70Side = $('input[name=S78Side]:checked').val();
            var S71Side = $('input[name=S79Side]:checked').val();
            var S72Side = $('input[name=S80Side]:checked').val();

            var isDivRight = $("#romrightdiv");
            var isDivLeft = $("#romleftdiv");
            isDivLeft.hide();
            isDivRight.hide();

            //alert(S64Side + "," + S65Side);
            if ((S64Side == "BOTH")
                || (S65Side == "BOTH")
                || (S66Side == "BOTH")
                || (S67Side == "BOTH")
                || (S68Side == "BOTH")
                || (S69Side == "BOTH")
                || (S70Side == "BOTH")
                || (S71Side == "BOTH")
                || (S72Side == "BOTH")
                ) {
                isDivLeft.show();
                isDivRight.show();
            }
            if ((S64Side == "LEFT")
                || (S65Side == "LEFT")
                || (S66Side == "LEFT")
                || (S67Side == "LEFT")
                || (S68Side == "LEFT")
                || (S69Side == "LEFT")
                || (S70Side == "LEFT")
                || (S71Side == "LEFT")
                || (S72Side == "LEFT")
                ) {
                isDivLeft.show();
            }
            if ((S64Side == "RIGHT")
                || (S65Side == "RIGHT")
                || (S66Side == "RIGHT")
                || (S67Side == "RIGHT")
                || (S68Side == "RIGHT")
                || (S69Side == "RIGHT")
                || (S70Side == "RIGHT")
                || (S71Side == "RIGHT")
                || (S72Side == "RIGHT")
                ) {
                isDivRight.show();
            }

        }
        $("input[type=radio]").change(function () {
            var Selectedvalue = $(this).val();
            setROM();
        });
