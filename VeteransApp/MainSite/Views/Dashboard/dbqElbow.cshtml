@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "Elbow";
    ViewData["Description"] = "Elbow";
}

@model MainSite.Models.ElbowModel


<div id="page-wrapper">
    <div class="col-xs-12">
        <h2 class="box-title m-b-0 m-t-40">Answer a Few Questions About Your Elbow Disability</h2>
        <p class="text-muted m-b-30 font-13"> Please answer each form section and click next.  It's best to answer these questions if you have access to your <a href="https://www.ebenefits.va.gov/" target="_blank">eBenefits</a>.  Each part is automatically saved when you finish it.</p>

        <form id="formelbow" method="post" action="/Dashboard/ElbowPost?contentStateID=6">
            <input id="UserID" name="UserID" class="form-control" type="hidden" value="@Model.UserID">
            @if (Model.ContentStateID == 6)
            {
                <button class="btn btn-success btn-lg btn-block text-uppercase waves-effect waves-light" type="submit">You Have Completed This Form Already, Click To Purchase</button>
                <hr />
            }
            <div class="panel-group col-lg-12" id="accordion">

                <!-- Part 1 -->
                <div class="panel panel-info">
                    <div class="panel-heading">
                        <h4 class="panel-title">
                            <a class="info" data-parent="#accordion" href="#collapse1">Part 1</a>
                        </h4>
                    </div>
                    <div id="collapse1" class="panel-collapse collapse in">
                        <div class="panel-body">
                            <div class="form-group m-t-20">
                                <label class="col-md-3">Full Name</label>
                                <div class="col-xs-4">
                                    <input id="NameOfPatient" name="NameOfPatient" class="form-control" type="text" required="" placeholder="Full Name" value="@Model.NameOfPatient">
                                </div>
                            </div>
                            <br />
                            <div class="form-group m-t-20">
                                <label class="col-md-3">Social Security Number</label>
                                <div class="col-xs-4">
                                    <input id="SocialSecurity" name="SocialSecurity" class="form-control" type="text" required="" placeholder="Social Security" value="@Model.SocialSecurity">
                                </div>
                            </div>
                            <br />
                            <hr />
                            <div>
                                <label class="font-bold">What is the diagnosis of your service-connected elbow condition?</label>
                                @Html.RadioButtonFor(x => x.Side, "LEFT") Left
                                @Html.RadioButtonFor(x => x.Side, "RIGHT") Right
                                @Html.RadioButtonFor(x => x.Side, "BOTH") Both
                            </div>
                            <hr />
                            <div class="form-group m-t-20">
                                <label class="font-bold">What is the diagnosis of your service-connected elbow condition? Check if applies and choose Right, Left or Both?</label>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S72) Olecranon Bursitis
                                    </label>
                                    @Html.RadioButtonFor(x => x.S72Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S72Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S72Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S73) Tricep Tendinitis
                                    </label>
                                    @Html.RadioButtonFor(x => x.S73Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S73Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S73Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S74) Lateral Epicondylitis
                                    </label>
                                    @Html.RadioButtonFor(x => x.S74Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S74Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S74Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S75) Medial Epicondylitis
                                    </label>
                                    @Html.RadioButtonFor(x => x.S75Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S75Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S75Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S76) Instability
                                    </label>
                                    @Html.RadioButtonFor(x => x.S76Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S76Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S76Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S77) Dislocation, Elbow
                                    </label>
                                    @Html.RadioButtonFor(x => x.S77Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S77Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S77Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S78) Osteoarthritis, Elbow
                                    </label>
                                    @Html.RadioButtonFor(x => x.S78Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S78Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S78Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S79) Total Elbow Arthroplasty
                                    </label>
                                    @Html.RadioButtonFor(x => x.S79Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S79Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S79Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S80) Ankylosis Of Elbow Joint
                                    </label>
                                    @Html.RadioButtonFor(x => x.S80Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S80Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S80Side, "BOTH") Both
                                </div>
                              
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S81Other) Other (specify)
                                    </label>
                                </div>

                            </div>
                            <hr />
                            <button class="btn btn-success btn-md pull-right" type="button" id="part1NextBtn">Next</button>
                        </div>
                    </div>
                </div>
                <!-- Part 1 -->
                <!-- Part 2 -->
                <div class="panel panel-info">
                    <div class="panel-heading">
                        <h4 class="panel-title">
                            <a class="info" data-parent="#accordion" href="#collapse2">Part 2</a>
                        </h4>
                    </div>
                    <div id="collapse2" class="panel-collapse collapse">
                        <div class="panel-body">
                            <div id="romrightdiv" class="form-check form-check-inline panel panel-primary">
                                <label class="font-bold">We will ask you to complete a series of Range of Motion test for the <spa>RIGHT</spa> elbow.</label>
                                <hr />
                                <label>Please perform the following Range of Motion for the <spa>RIGHT</spa> elbow.</label>
                                <br />
                                <span>Flexion (normal endpoint = 145 degrees)</span>
                                @Html.DropDownListFor(x => x.S130, (SelectList)ViewBag.MovementList145Deg)
                                <br />
                                <span>Extension</span>
                                @Html.DropDownListFor(x => x.S134, (SelectList)ViewBag.MovementList180Deg)
                                <br />
                                <span>Forarm Supination</span>
                                @Html.DropDownListFor(x => x.S137, (SelectList)ViewBag.MovementList85Deg)
                                <br />
                                <span>Forarm Pronation</span>
                                @Html.DropDownListFor(x => x.S140, (SelectList)ViewBag.MovementList80Deg)

                                <hr />
                                <label>Please perform the following Range of Motion 3 repetitions for the <spa>RIGHT</spa> elbow and record the last repetition below.</label>
                                <br />
                                <span>Flexion (normal endpoint = 145 degrees)</span>
                                @Html.DropDownListFor(x => x.S170, (SelectList)ViewBag.MovementList145Deg)
                                <br />
                                <span>Extension</span>
                                @Html.DropDownListFor(x => x.S169, (SelectList)ViewBag.MovementList180Deg)
                                <br />
                                <span>Forarm Supination</span>
                                @Html.DropDownListFor(x => x.S171, (SelectList)ViewBag.MovementList85Deg)
                                <br />
                                <span>Forarm Pronation</span>
                                @Html.DropDownListFor(x => x.S172, (SelectList)ViewBag.MovementList80Deg)

                                <hr />
                                <label>For the <spa>RIGHT</spa> elbow at its worst or during a flare-up, what is the range for each motion?</label>
                                <br />
                                <span>Flexion (normal endpoint = 145 degrees)</span>
                                @Html.DropDownListFor(x => x.S292, (SelectList)ViewBag.MovementList145Deg)
                                <br />
                                <span>Extension</span>
                                @Html.DropDownListFor(x => x.S290, (SelectList)ViewBag.MovementList180Deg)
                                <br />
                                <span>Forarm Supination</span>
                                @Html.DropDownListFor(x => x.S288, (SelectList)ViewBag.MovementList85Deg)
                                <br />
                                <span>Forarm Pronation</span>
                                @Html.DropDownListFor(x => x.S286, (SelectList)ViewBag.MovementList80Deg)

                            </div>

                            <div id="romleftdiv" class="form-check form-check-inline panel panel-primary">
                                <label class="font-bold">We will ask you to complete a series of Range of Motion test for the <spa>LEFT</spa> elbow.</label>
                                <hr />
                                <label>Please perform the following Range of Motion for the <spa>LEFT</spa> elbow.</label>
                                <br />
                                <span>Flexion (normal endpoint = 145 degrees)</span>
                                @Html.DropDownListFor(x => x.S122, (SelectList)ViewBag.MovementList145Deg)
                                <br />
                                <span>Extension</span>
                                @Html.DropDownListFor(x => x.S112, (SelectList)ViewBag.MovementList180Deg)
                                <br />
                                <span>Forarm Supination</span>
                                @Html.DropDownListFor(x => x.S113, (SelectList)ViewBag.MovementList85Deg)
                                <br />
                                <span>Forarm Pronation</span>
                                @Html.DropDownListFor(x => x.S116, (SelectList)ViewBag.MovementList80Deg)

                                <hr />
                                <label>Please perform the following Range of Motion 3 repetitions for the <spa>LEFT</spa> elbow and record the last repetition below.</label>
                                <br />
                                <span>Flexion (normal endpoint = 145 degrees)</span>
                                @Html.DropDownListFor(x => x.S154, (SelectList)ViewBag.MovementList145Deg)
                                <br />
                                <span>Extension</span>
                                @Html.DropDownListFor(x => x.S155, (SelectList)ViewBag.MovementList180Deg)
                                <br />
                                <span>Forarm Supination</span>
                                @Html.DropDownListFor(x => x.S148, (SelectList)ViewBag.MovementList85Deg)
                                <br />
                                <span>Forarm Pronation</span>
                                @Html.DropDownListFor(x => x.S147, (SelectList)ViewBag.MovementList80Deg)

                                <hr />
                                <label>For the <spa>LEFT</spa> elbow at its worst or during a flare-up, what is the range for each motion?</label>
                                <br />
                                <span>Flexion (normal endpoint = 145 degrees)</span>
                                @Html.DropDownListFor(x => x.S260, (SelectList)ViewBag.MovementList145Deg)
                                <br />
                                <span>Extension</span>
                                @Html.DropDownListFor(x => x.S255, (SelectList)ViewBag.MovementList180Deg)
                                <br />
                                <span>Forarm Supination</span>
                                @Html.DropDownListFor(x => x.S253, (SelectList)ViewBag.MovementList85Deg)
                                <br />
                                <span>Forarm Pronation</span>
                                @Html.DropDownListFor(x => x.S254, (SelectList)ViewBag.MovementList80Deg)

                            </div>     
                            <hr />                       
                            <button class="btn btn-warning btn-md pull-left" type="button" id="part2PrevBtn">Prev</button>
                            <button class="btn btn-success btn-md pull-right" type="button" id="part2NextBtn">Next</button>
                        </div>
                    </div>
                </div>
                <!-- Part 2 -->
                <!-- Part 3 -->
                @*<div class="panel panel-info">
                    <div class="panel-heading">
                        <h4 class="panel-title">
                            <a class="info" data-parent="#accordion" href="#collapse3">Part 3</a>
                        </h4>
                    </div>
                    <div id="collapse3" class="panel-collapse collapse">
                        <div class="panel-body">
                            <button class="btn btn-warning btn-md pull-left" type="button" id="part3PrevBtn">Prev</button>
                            <button class="btn btn-success btn-md pull-right" type="button" id="part3NextBtn">Next</button>
                        </div>
                    </div>
                </div>*@
                <!-- Part 3 -->
                <!-- Part 4 -->
                @*<div class="panel panel-info">
                    <div class="panel-heading">
                        <h4 class="panel-title">
                            <a class="info" data-parent="#accordion" href="#collapse4">Part 4</a>
                        </h4>
                    </div>
                    <div id="collapse4" class="panel-collapse collapse">
                        <div class="panel-body">
                            <button class="btn btn-warning btn-md pull-left" type="button" id="part4PrevBtn">Prev</button>
                            <button class="btn btn-success btn-md pull-right" type="button" id="part4NextBtn">Next</button>
                        </div>
                    </div>
                </div>*@
                <!-- Part 4 -->
                <!-- Finish -->
                <div class="panel panel-info">
                    <div class="panel-heading">
                        <h4 class="panel-title">
                            <a class="info" data-parent="#accordion" href="#collapsef">Finished</a>
                        </h4>
                    </div>
                    <div id="collapsef" class="panel-collapse collapse">
                        <div class="panel-body">
                            <h2 class="box-title m-b-0 m-t-40">Thank you for completing the questions.  Click submit to process.</h2>
                            <button class="btn btn-warning btn-md pull-left" type="button" id="finishPrevBtn">Prev</button>
                            <button class="btn btn-success btn-md pull-right" type="submit" id="finishBtn">Submit</button>
                        </div>
                    </div>
                </div>
                <!-- Finish -->



            </div>

        </form>
    </div>
</div>

