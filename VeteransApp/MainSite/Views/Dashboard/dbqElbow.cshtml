                                <br />
                                <span>Extension</span>
                                @Html.DropDownListFor(x => x.S155, (SelectList)ViewBag.MovementList0To110Deg)
                                <br />
                                <span>Forarm Supination</span>
                                @Html.DropDownListFor(x => x.S148, (SelectList)ViewBag.MovementList85Deg)
                                <br />
                                <span>Forarm Pronation</span>
                                @Html.DropDownListFor(x => x.S147, (SelectList)ViewBag.MovementList80Deg)

                                <hr />
                                <label>For the <spa>LEFT</spa> elbow at its worst or during a flare-up, what is the range for each motion?</label>
