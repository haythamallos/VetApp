        function validate_1() {
            var isValid = true;
            var s;

            s = jQuery('input[name="NameOfPatient"]').val();
            if ((s == '') || s == null) {
                isValid = false;
            }

            s = jQuery('input[name="SocialSecurity"]').val();
            if ((s == '') || s == null) {
                isValid = false;
            }

            return isValid;
        }

        function validate_2() {
            var isValid = true;

