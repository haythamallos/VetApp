@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "Wrist";
    ViewData["Description"] = "Wrist";
}

@model MainSite.Models.WristModel

<style>
    .panel-heading {
        cursor: pointer;
    }

    .clickable {
        cursor: pointer;
    }

        .clickable .glyphicon {
            background: rgba(0, 0, 0, 0.15);
            display: inline-block;
            padding: 6px 12px;
            border-radius: 4px;
        }

    .panel-heading span {
        margin-top: -23px;
        font-size: 15px;
        margin-right: -9px;
    }

    a.clickable {
        color: inherit;
    }

        a.clickable:hover {
            text-decoration: none;
        }
</style>

<div id="page-wrapper">
    <div class="col-xs-12">
        <h2 class="box-title m-b-0 m-t-40">Answer a Few Questions About Your Wrist Disability</h2>
        <p class="text-muted m-b-30 font-13"> Please answer each form section and click next.  It's best to answer these questions if you have access to your <a href="https://www.ebenefits.va.gov/" target="_blank">eBenefits</a>.  Each part is automatically saved when you finish it.</p>

        <form id="formwrist" method="post" action="/Dashboard/WristPost?contentStateID=6" target="_blank">
            <input id="UserID" name="UserID" class="form-control" type="hidden" value="@Model.UserID">
            @*@if (Model.ContentStateID == 6)
            {
                <button class="btn btn-success btn-lg btn-block text-uppercase waves-effect waves-light" type="submit">You Have Completed This Form Already, Click To Purchase</button>
                <hr />
            }*@
            <div class="panel-group col-lg-12" id="accordion">

                <!-- Section 1 -->
                <div class="panel panel-info">
                    <div class="panel-heading" id="part1Show">
                        <h4 class="panel-title">
                            <a class="info" data-parent="#accordion" href="#collapse1">Section 1</a>
                        </h4>
                    </div>
                    <div id="collapse1" class="panel-collapse collapse in">
                        <div class="panel-body">
                            <div class="form-group m-t-20">
                                <label class="col-md-3">Full Name</label>
                                <div class="col-xs-4">
                                    @Html.EditorFor(x => x.NameOfPatient, new { htmlAttributes = new { @class = "form-control", placeholder = "Full Name"} })
                                </div>
                            </div>
                            <br />
                            <div class="form-group m-t-20">
                                <label class="col-md-3">Social Security Number</label>
                                <div class="col-xs-4">
                                    @Html.EditorFor(x => x.SocialSecurity, new { htmlAttributes = new { @class = "form-control", placeholder = "Social Security" } })
                                </div>
                            </div>
                            <br />
                            <hr />
                            <div class="form-group m-t-20">
                                <div class="row">
                                    <div class="col-md-5">
                                        <span>Please type in the diagnosis of your service-connected disability exactly as it is shown on your rating:  </span>
                                    </div>
                                    <div class="col-md-7">
                                        @Html.TextAreaFor(x => x.S17, new { htmlAttributes = new { @class = "form-control form-control-line", placeholder = "enter your text here ..." }, style = "width: 100%; height: 100px" })
                                    </div>
                                </div>

                                <label class="font-bold">What is the diagnosis of your service-connected wrist condition? Check if applies and choose Right, Left or Both?</label>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S64) Wrist Sprain, Chronic
                                    </label>
                                    @Html.RadioButtonFor(x => x.S64Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S64Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S64Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S65) Tendonitis, Wrist
                                    </label>
                                    @Html.RadioButtonFor(x => x.S65Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S65Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S65Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S66) Ganglion Cyst
                                    </label>
                                    @Html.RadioButtonFor(x => x.S66Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S66Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S66Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S67) Carpal Metacarpal, Arthritis
                                    </label>
                                    @Html.RadioButtonFor(x => x.S67Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S67Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S67Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S68) Osteoarthritis, Arthtritis, Wrist
                                    </label>
                                    @Html.RadioButtonFor(x => x.S68Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S68Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S68Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S69) DeQuervains Syndrome
                                    </label>
                                    @Html.RadioButtonFor(x => x.S69Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S69Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S69Side, "BOTH") Both
