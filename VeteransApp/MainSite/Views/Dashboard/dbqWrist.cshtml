                                    @Html.RadioButtonFor(x => x.S103Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S103Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S103Side, "BOTH") Both
                                    <br />
