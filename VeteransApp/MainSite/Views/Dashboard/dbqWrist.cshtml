                                    @Html.RadioButtonFor(x => x.S81Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S81Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S81Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S87) Ankylosis Of Wrist
                                    </label>
                                    @Html.RadioButtonFor(x => x.S87Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S87Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S87Side, "BOTH") Both
                                </div>
