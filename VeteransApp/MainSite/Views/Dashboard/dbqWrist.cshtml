                                    @Html.RadioButtonFor(x => x.S71Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S71Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S71Side, "BOTH") Both
