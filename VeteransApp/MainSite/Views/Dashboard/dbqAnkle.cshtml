<script type="text/javascript">

    $('document').ready(function () {

        function setROM() {
            var S64Side = $('input[name=S82Side]:checked').val();
            var S65Side = $('input[name=S83Side]:checked').val();
            var S66Side = $('input[name=S84Side]:checked').val();
            var S67Side = $('input[name=S85Side]:checked').val();
