            var S71Side = $('input[name=S89Side]:checked').val();
            var S72Side = $('input[name=S90Side]:checked').val();
            var S81Side = $('input[name=S91Side]:checked').val();
            var S111Side = $('input[name=S92Side]:checked').val();
            var S93Side = $('input[name=S93Side]:checked').val();

            var isDivRight = $("#romrightdiv");
            var isDivLeft = $("#romleftdiv");
            isDivLeft.hide();
            isDivRight.hide();

