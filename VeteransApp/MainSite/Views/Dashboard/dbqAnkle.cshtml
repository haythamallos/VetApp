@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "Ankle";
    ViewData["Description"] = "Ankle";
}

@model MainSite.Models.AnkleModel

<style>
    .panel-heading {
        cursor: pointer;
    }

    .clickable {
        cursor: pointer;
    }

        .clickable .glyphicon {
            background: rgba(0, 0, 0, 0.15);
            display: inline-block;
            padding: 6px 12px;
            border-radius: 4px;
        }

    .panel-heading span {
        margin-top: -23px;
        font-size: 15px;
        margin-right: -9px;
    }

    a.clickable {
        color: inherit;
    }

        a.clickable:hover {
            text-decoration: none;
        }
</style>

<div id="page-wrapper">
    <div class="col-xs-12">
        <h2 class="box-title m-b-0 m-t-40">Answer a Few Questions About Your Ankle Disability</h2>
        <p class="text-muted m-b-30 font-13"> Please answer each form section and click next.  It's best to answer these questions if you have access to your <a href="https://www.ebenefits.va.gov/" target="_blank">eBenefits</a>.  Each part is automatically saved when you finish it.</p>

        <form id="formankle" method="post" action="/Dashboard/AnklePost?contentStateID=6" target="_blank">
            <input id="UserID" name="UserID" class="form-control" type="hidden" value="@Model.UserID">
            @*@if (Model.ContentStateID == 6)
                {
                    <button class="btn btn-success btn-lg btn-block text-uppercase waves-effect waves-light" type="submit">You Have Completed This Form Already, Click To Purchase</button>
                    <hr />
                }*@
            <div class="panel-group col-lg-12" id="accordion">

                <!-- Section 1 -->
                <div class="panel panel-info">
                    <div class="panel-heading" id="part1Show">
                        <h4 class="panel-title">
                            <a class="info" data-parent="#accordion" href="#collapse1">Section 1</a>
                        </h4>
                    </div>
                    <div id="collapse1" class="panel-collapse collapse in">
                        <div class="panel-body">
                            <div class="form-group m-t-20">
                                <label class="col-md-3">Full Name</label>
                                <div class="col-xs-4">
                                    @Html.EditorFor(x => x.NameOfPatient, new { htmlAttributes = new { @class = "form-control", placeholder = "Full Name" } })
                                </div>
                            </div>
                            <br />
                            <div class="form-group m-t-20">
                                <label class="col-md-3">Social Security Number</label>
                                <div class="col-xs-4">
                                    @Html.EditorFor(x => x.SocialSecurity, new { htmlAttributes = new { @class = "form-control", placeholder = "Social Security" } })
                                </div>
                            </div>
                            <br />
                            <hr />
                            <div class="form-group m-t-20">
                                <div class="row">
                                    <div class="col-md-5">
                                        <span>Please type in the diagnosis of your service-connected disability exactly as it is shown on your rating:  </span>
                                    </div>
                                    <div class="col-md-7">
                                        @Html.TextAreaFor(x => x.S97, new { htmlAttributes = new { @class = "form-control form-control-line", placeholder = "enter your text here ..." }, style = "width: 100%; height: 100px" })
                                    </div>
                                </div>

                                <label class="font-bold">What is the diagnosis of your service-connected ankle condition? Check if applies and choose Right, Left or Both?</label>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S82) Lateral collateral ligament sprain (chronic/recurrent)
                                    </label>
                                    @Html.RadioButtonFor(x => x.S82Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S82Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S82Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S83) Deltoid ligament sprain (chronic/recurrent)
                                    </label>
                                    @Html.RadioButtonFor(x => x.S83Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S83Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S83Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S84) Osteochondritis dissecans to include osteochondral fracture
                                    </label>
                                    @Html.RadioButtonFor(x => x.S84Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S84Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S84Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S85) Impingement
                                    </label>
                                    @Html.RadioButtonFor(x => x.S85Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S85Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S85Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S86) Tendonitis
                                    </label>
                                    @Html.RadioButtonFor(x => x.S86Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S86Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S86Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S87) Retrocalcaneal bursitis
                                    </label>
                                    @Html.RadioButtonFor(x => x.S87Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S87Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S87Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S88) Achiles tendon rupture
                                    </label>
                                    @Html.RadioButtonFor(x => x.S88Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S88Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S88Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S89) Osteoarthritis of the ankle
                                    </label>
                                    @Html.RadioButtonFor(x => x.S89Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S89Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S89Side, "BOTH") Both
                                </div>
                                <div class="form-check form-check-inline">
                                    <label class="form-check-label font-normal">
                                        @Html.EditorFor(x => x.S90) Avascular necrosis, talus
                                    </label>
                                    @Html.RadioButtonFor(x => x.S90Side, "RIGHT") Right
                                    @Html.RadioButtonFor(x => x.S90Side, "LEFT") Left
                                    @Html.RadioButtonFor(x => x.S90Side, "BOTH") Both
                                </div>
