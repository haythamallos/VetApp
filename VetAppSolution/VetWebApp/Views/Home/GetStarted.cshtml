
@{
    ViewData["Title"] = "Get Started Earning More Benefits";
}
<div class="estimated_sec">
    <h2>Estimated Benefits</h2>
    <div class="estimated_sec_inner">
    </div>
</div>

<section>
   
    <form role="form">
        <center>


            <div class="form-group">
                <div class="form-group">
                    <label>Is this your first time filing?</label>
                    <div class="radio">
                        <label>
                            <input type="radio" name="optionsRadios1" id="optionsRadios1" value="optionYes" checked>Yes
                        </label>
                        <label>
                            <input type="radio" name="optionsRadios1" id="optionsRadios2" value="optionNo">No
                        </label>
                    </div>
                </div>
                <div class="form-group">
                    <label>Do you have a claim in with the VA?</label>
                    <div class="radio">
                        <label>
                            <input type="radio" name="optionsRadios2" id="optionsRadios3" value="optionYes" checked>Yes
                        </label>
                        <label>
                            <input type="radio" name="optionsRadios2" id="optionsRadios4" value="optionNo">No
                        </label>
                    </div>
                </div>
                <div class="form-group">
                    <label>Do you have an active appeal?</label>
                    <div class="radio">
                        <label>
                            <input type="radio" name="optionsRadios3" id="optionsRadios5" value="optionYes" checked>Yes
                        </label>
                        <label>
                            <input type="radio" name="optionsRadios3" id="optionsRadios6" value="optionNo">No
                        </label>
                    </div>
                </div>
                <div class="form-group">
                    <label>Do you have a rating by the VA?</label>
                    <div class="radio">
                        <label>
                            <input type="radio" name="optionsRadios4" id="optionsRadios7" value="optionYes" checked>Yes
                        </label>
                        <label>
                            <input type="radio" name="optionsRadios4" id="optionsRadios8" value="optionNo">No
                        </label>
                    </div>
                </div>
                <div class="form-group">
                    <label>What is your current rate?</label>
                    <div class="form-group" style="width: 100px;">
                        <input class="form-control">
                        <p class="help-block">Example 20%</p>
                    </div>
                </div>
            </div>
            <button type="submit" class="btn btn-default">Submit Button</button>
            <button type="reset" class="btn btn-default">Reset Button</button>
        </center>
    </form>
</section>