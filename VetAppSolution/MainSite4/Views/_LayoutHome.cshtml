    </div>

    <header id="header">
        <!--header-->
        <div class="header_top">
            <!--header_top-->
            <div class="container">
                <div class="row">
                    <div class="col-sm-9">

                        <div class="contactinfo">
                            <ul class="nav nav-pills">
                                <li><a href="/Home"><i class="fa fa-home"></i> Home</a></li>
                                <li><a href="#"><i class="fa fa-phone"></i> 619-822-2070</a></li>
                                <li><a href="javascript:void(0)" id="btnContactFormModal"><i class="fa fa-envelope"></i> info@vvcservices.com</a></li>
                                <li><a href="#" onclick="showform('captureformdiv');"><i class="fa fa-star"></i> Free Evaluation</a></li>
                                <li><a href="#" onclick="showform('loginformdiv');"><i class="fa fa-sign-in"></i> Sign in</a></li>
                                <li><a href="#" onclick="showform('registerformdiv');"><i class="fa fa-long-arrow-up"></i> Sign Up</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="social-icons pull-right">
                            <ul class="nav navbar-nav">
                                @if (User.Identity.IsAuthenticated)
                                {
                                    <li><a asp-controller="Account" asp-action="Logout">Logout</a></li>
                                }
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div><!--/header_top-->
    </header>


    @RenderBody()

    <!-- jQuery -->
    <script src="~/plugins/bower_components/jquery/dist/jquery.min.js"></script>
